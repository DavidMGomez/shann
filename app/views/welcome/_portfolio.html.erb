<section id="portfolio">
   <div class="container">
      <div class="box first">
         <div class="row">
            <div class="center gap">
               <h2>Encuentre al profesional ideal</h2>
               <h3>Pude escoger profesionales entre diversas áreas de su interés.</h3>
                     <!--Ciudades-->
                            <div class="col-md-4 col-sm-6" >
                                 <h3>Ubicacion</h3>
                                 <ul class="list-group">
                                <a class="list-group-item" href="#" >Bogota</a>
                                <a class="list-group-item"  href="#">Medellin</a>
                                 <a class="list-group-item" href="#" >Cali</a>
                                <a class="list-group-item"  href="#">Cartagena</a>
                                 <a class="list-group-item" href="#" >Barranquilla</a>
                                <a class="list-group-item"  href="#">Cucuta</a>
                                <a class="list-group-item btn btn-primary" style="color: white;" href="#">Escoger otra ubicacion</a>
                                <ul>
                            </div>
                     <!--/ciudades-->
                      <!--Categorias-->
                            <div class="col-md-8 col-sm-6">
                                <h3>Categoria</h3>
                                 
                                 <div class="col-md-6 col-sm-6">
                                        <ul class="list-group">
                                            <%for i in 1..6%>
                                                 <a class="list-group-item" href="#" >Categoria <%=i%></a>         
                                            <%end%>
                                        </ul>
                                    </div>
                                   
                                  <div class="col-md-6 col-sm-6">
